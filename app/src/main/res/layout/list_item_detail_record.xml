<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="userId"
            type="Integer" />

        <variable
            name="record"
            type="com.infinity.omos.data.record.DetailRecordModel" />
    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            android:id="@+id/layout_detail_record"
            layout="@layout/layout_detail_record"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:record="@{record}"
            app:userId="@{userId}" />

    </FrameLayout>

</layout>